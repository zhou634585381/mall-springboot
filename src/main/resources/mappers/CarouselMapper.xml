<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mall.mapper.CarouselMapper">
    <resultMap id="CarouselMap" type="com.example.mall.entity.Carousel">
        <result column="carousel_id" property="carouselId" jdbcType="INTEGER"/>
        <result column="img_path" property="imgPath" jdbcType="VARCHAR"/>
        <result column="describes" property="describes" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="selectAll" resultMap="CarouselMap">
        SELECT
        *
        FROM
        carousel
    </select>

</mapper>
